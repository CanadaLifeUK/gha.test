{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "yyyy'-'MM'-'dd'T'HH':'mm':'ss.fffffff ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  },
  "AllowedHosts": "*",
  "ApplicationSettings": {
    "DatabaseSettings": {
      "DataSource": "",
      "DataSourceProperties": "Initial Catalog=<CHANGE>;Integrated Security=SSPI;TrustServerCertificate=True;MultipleActiveResultSets=True",
      "Password": "",
      "Username": ""
    }
  }
}
